{
  "scripts": {
    "add0": "git remote add origin git@github.com:MatthPerez/OnvaSortir.git",
    "add": "git add *",
    "push": "git push git@github.com:MatthPerez/OnvaSortir.git",
    "pull": "git pull origin main",
